[Unit]
Description=NS Server Configuration Tool
After=network.target
Wants=network.target

[Service]
Type=simple
User=root
Group=root
WorkingDirectory=/usr/share/ns
ExecStart=/usr/bin/ns service enable app
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

NoNewPrivileges=no
ProtectSystem=no
ProtectHome=no
PrivateTmp=no
PrivateDevices=no
ProtectKernelTunables=no
ProtectKernelModules=no
ProtectControlGroups=no

[Install]
WantedBy=multi-user.target